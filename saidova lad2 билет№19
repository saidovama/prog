#include <stdio.h>

int main() {
    int a, b, c;
    printf ("Введите сторону треугольника:");
    scanf("%d",&a);
    printf ("Введите сторону треугольника:");
    scanf("%d",&b);
    printf ("Введите сторону треугольника:");
    scanf("%d",&c);
    if (a+b>c && a+c>b && b+c>a) {printf ("Является треугольником");
    }
      else {printf ("Не является треугольником");
    };
    return 0;
}
