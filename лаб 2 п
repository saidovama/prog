import math

# Ввод данных
V = float(input("Введите начальную скорость (V): "))
alpha = float(input("Введите угол α (в градусах): "))
g = 9.81  # Ускорение свободного падения

# Перевод угла из градусов в радианы
alpha_radians = math.radians(alpha)

# Вычисление дальности полета L
L = (V**2 * math.sin(2 * alpha_radians)) / g

# Вывод результата
print(f"Дальность полета L: {L:.2f} м")
