#include <iostream>
#include <cmath>
using namespace std;

int main() {
    double V, alpha, g = 9.81;

    // Ввод данных
    cout << "Введите начальную скорость (V): ";
    cin >> V;
    cout << "Введите угол α (в градусах): ";
    cin >> alpha;

    // Перевод угла из градусов в радианы
    double alpha_radians = alpha * M_PI / 180;

    // Вычисление дальности полета L
    double L = (pow(V, 2) * sin(2 * alpha_radians) / g;

    // Вывод результата
    cout << "Дальность полета L: " << L << " м" << endl;

    return 0;
}
