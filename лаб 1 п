import math

# Ввод данных
V = float(input("Введите начальную скорость (V): "))
T = float(input("Введите время полета (T): "))
g = 9.81  # Ускорение свободного падения

# Вычисление угла
a = math.asin((g * T) / (2 * V))

# Перевод угла из радиан в градусы
a_degrees = math.degrees(a)

# Вывод результата
print(f"Угол α (в радианах): {a:.2f}")
print(f"Угол α (в градусах): {a_degrees:.2f}")
